<mvc:View controllerName="my.sap_coder_agro_delivery_manager.controller.TransportationList" xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic" xmlns:footerbar="sap.ushell.ui.footerbar"
	xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:unif="sap.ui.unified">
	<semantic:MasterPage title="Transportation List" showNavButton="true" navButtonPress="onNavBack">
		<semantic:content>
			<PullToRefresh id="pullToRefresh" visible="{device>/support/touch}" refresh="onRefresh"/>
			<Table inset="false" items="{ path: '/Transportations' }">
				<columns>
					<Column width="12em">
						<Text text="Transportation"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem press="onTransportationDetailsNav" type="Navigation">
						<cells>
							<ObjectIdentifier title="{TransportationNum}" text="{TransportationNum}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</semantic:content>
	</semantic:MasterPage>
</mvc:View>